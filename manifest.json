{
  "manifest_version": 3,
  "name": "Enprod",
  "description": "Enhance productivity on Internet",
  "version": "1.0",
  "action": {
    "default_icon": "hello_extensions.png"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://*/*"
  ]
}
